{
  "name": "Stop Mod Reposts",
  "version": "5.0.1",
  "description": "An extention that warns you when you go on a mod repost site",
  "default_locale": "en",
  "author": "berrysauce",
  "icons": {
    "128": "icons/icon128.png",
    "48": "icons/icon48.png",
    "32": "icons/icon32.png",
    "16": "icons/icon16.png"
  },
  "background": {
    "scripts": ["out/background.bundle.js"]
  },
  "permissions": ["https://api.stopmodreposts.org/sites.json", "tabs"],
  "manifest_version": 2
}
